{
  "name": "@luminafe/cli",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lumina-upload": "dist/index.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "node --conditions=source --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts --help",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepack": "pnpm run build",
    "pack:dry": "npm_config_cache=/tmp/.npm-cache npm pack --dry-run"
  },
  "dependencies": {
    "citty": "^0.2.1",
    "exifr": "^7.1.3",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "~5.8.2"
  }
}
